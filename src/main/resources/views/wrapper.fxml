<StackPane prefHeight="834.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.gleidsonmt.dashboardfx.controllers.WrapperController">
   <stylesheets>
      <URL value="@../core.app/css/colors.css" />
      <URL value="@../core.app/css/skeleton.css" />
      <URL value="@../core.app/css/typographic.css" />
      <URL value="@../core.app/css/bootstrap.css" />
   </stylesheets>
   <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
      <HBox>
         <VBox fx:id="bodyContent" alignment="CENTER" minHeight="850.0" spacing="10.0">
            <padding>
               <Insets bottom="10.0" left="30.0" right="30.0" top="30.0" />
            </padding>
            <VBox VBox.vgrow="ALWAYS">
               <children>
                  <Label prefHeight="38.0" prefWidth="195.0" text="PopupCreator">
                     <styleClass>
                        <String fx:value="h3" />
                        <String fx:value="title" />
                     </styleClass>
                  </Label>
                  <TextFlow prefHeight="107.0" prefWidth="938.0">
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: -text-color;" text="PopupCreator is an accessor class for creating dialogs, alerts and side drawers." wrappingWidth="558.283203125" />
                  </TextFlow>
                  <Label style="-fx-font-weight: bold;" text="Drawer">
                     <styleClass>
                        <String fx:value="h4" />
                        <String fx:value="title" />
                     </styleClass>
                  </Label>
                  <TextFlow>
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: -text-color;" styleClass="text-14" text="Create a left drawer or right using this.&#10;PopupCreator.INSTANCE.createDrawerLeft(new VBox());&#10;&#10;" wrappingWidth="558.283203125" />
