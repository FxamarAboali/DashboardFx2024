                  </TextFlow>
                  <HBox>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                     <children>
                        <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#openDrawerLeft" prefHeight="12.0" prefWidth="126.0" styleClass="btn-outlined" text="Create Drawer Left" />
                        <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#openDrawerRight" prefHeight="12.0" prefWidth="126.0" styleClass="btn-outlined" text="Create Drawer Right" />
                     </children>
                  </HBox>
                  <Label layoutX="10.0" layoutY="102.0" style="-fx-font-weight: bold;" text="Alert">
                     <styleClass>
                        <String fx:value="h4" />
                        <String fx:value="title" />
                     </styleClass>
                  </Label>
                  <TextFlow layoutX="10.0" layoutY="140.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: -text-color;" styleClass="text-14" text="Create multiples alerts using this.&#10;PopupCreator.INSTANCE.createAlert(AlertType.INFO, &quot;Title&quot;, &quot;Message&quot;, new Button(&quot;Ok');&#10;&#10;&#10;" wrappingWidth="558.283203125" />
                     </children>
                  </TextFlow>
                  <TilePane>
                     <children>
                        <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#createAlertInfo" prefHeight="12.0" prefWidth="126.0" styleClass="btn-outlined" text="Create Alert Info" />
                        <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#createAlertError" prefHeight="12.0" prefWidth="126.0" styleClass="btn-outlined" text="Create Alert Error" />
                        <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#createAlertWarning" prefHeight="40.0" prefWidth="128.0" styleClass="btn-outlined" text="Create Alert Warning" />
                        <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#createAlertDone" prefHeight="12.0" prefWidth="126.0" styleClass="btn-outlined" text="Create Alert Done" />
                     </children>
                  </TilePane>
                  <Label layoutX="10.0" layoutY="275.0" prefWidth="99.0" style="-fx-font-weight: bold;" text="Dialogs">
                     <styleClass>
                        <String fx:value="h4" />
                        <String fx:value="title" />
                     </styleClass>
                  </Label>
                  <TextFlow layoutX="10.0" layoutY="313.0">
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: -text-color;" styleClass="text-14" text="Create a simple dialog.&#10;PopupCreator.INSTANCE.createAlert(&quot;Title&quot;, &quot;Message&quot;, new Button(&quot;OK&quot;);&#10;&#10;&#10;" wrappingWidth="558.283203125" />
                     <Button minHeight="40.0" minWidth="100.0" mnemonicParsing="false" onAction="#createDialog" prefHeight="12.0" prefWidth="126.0" styleClass="btn-outlined" text="Create Dialog" />
                  </TextFlow>
               </children>
            </VBox>
            <VBox prefWidth="726.0" spacing="20.0">
               <HBox alignment="BOTTOM_LEFT">
                  <Label style="-fx-font-size: 8pt;" styleClass="min" text="Author" />
                  <Separator prefHeight="0.0" prefWidth="197.0" HBox.hgrow="ALWAYS" />
               </HBox>
               <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" VBox.vgrow="ALWAYS">
                  <padding>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
                  <Label text="Gleidson Neves da Silveira" />
               </HBox>
            </VBox>
         </VBox>
         <VBox>
            <children>
               <WebView fx:id="webView" minWidth="350.0" prefHeight="850.0" prefWidth="144.0" />
            </children>
            <padding>
               <Insets top="40.0" />
            </padding>
         </VBox>
      </HBox>
   </ScrollPane>
</StackPane>
